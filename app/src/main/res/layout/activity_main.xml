<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btnTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnTest2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="web-ui"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnTest3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="视频全屏播放" />

    <Button
        android:id="@+id/btnTest4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试 window.alert" />

    <Button
        android:id="@+id/btnTest5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试 window.prompt" />

    <Button
        android:id="@+id/btnTest6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试 window.confirm" />

    <Button
        android:id="@+id/btnTest7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试 下载文件" />

    <Button
        android:id="@+id/btnTest8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试 传入一个初始标题" />

    <Button
        android:id="@+id/btnTest9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试 使用单独的webModule" />

    <Button
        android:id="@+id/btnTest10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="隐藏 toolbar" />
</LinearLayout>