<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Widget.UniWeb.Toolbar.Surface" parent="Widget.MaterialComponents.Toolbar.Surface" tools:ignore="ResourceName">
        <item name="contentInsetStartWithNavigation">0dp</item>
        <item name="navigationIcon">@drawable/uniweb_action_bar_back_icon</item>
        <item name="materialThemeOverlay">@style/ThemeOverlay.UniWeb.Toolbar.Surface</item>
        <item name="titleTextColor">@color/uniweb_textColorPrimary</item>
        <item name="subtitleTextColor">@color/uniweb_textColorSecondary</item>
    </style>

    <style name="ThemeOverlay.UniWeb.Toolbar.Surface" parent="" tools:ignore="ResourceName">
        <item name="colorControlNormal">@color/uniweb_colorOnSurface</item>
        <item name="actionMenuTextColor">@color/uniweb_colorOnSurface</item>
    </style>

    <style name="Widget.UniWeb.AppBarLayout" parent="Widget.Design.AppBarLayout" tools:ignore="ResourceName">
        <item name="android:background">@null</item>
        <item name="elevation">0dp</item>
    </style>

</resources>